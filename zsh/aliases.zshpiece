alias reload!='. ~/.zshrc'
alias ls='gls -F --color'
alias l='ls -1'
alias la='ls -a'
alias ll='ls -lh'
alias lla='ls -lha'
alias lr='ls -1ctr'
alias grep='grep --colour'
alias apt-i="sudo apt-get install"
alias apt-s="apt-cache search"
alias 'ps?'='ps -ef | grep -v grep | grep "\\<PID"; ps -ef | grep -v grep | grep '
